{
  "name": "zorro-bot",
  "alias": "zorro-bot",
  "scale": {
    "all": {
      "min": 1,
      "max": 2
    }
  },
  "env": {
    "LOG_LEVEL": "info",
    "APP_ID": "@zorro-bot-app-id",
    "PRIVATE_KEY": "@zorro-bot-private-key",
    "WEBHOOK_SECRET": "@zorro-bot-webhook-secret",
    "DINGTALK_TOKEN": "@zorro-bot-dingtalk-token"
  }
}
